        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidBatch1"
        tools:targetApi="31">
        <activity
            android:name=".NavDemoActivity"
            android:exported="false"
            android:label="@string/title_activity_nav_demo" />
        <activity
            android:name=".BottomNavActivity"
            android:exported="false"
            android:theme="@style/bottomNavTheme" />
        <activity
            android:name=".TabDemoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityToFragmentActivity"
            android:exported="false" />
        <activity
            android:name=".SubCategoryCouponActivity"
            android:exported="false" />
        <activity
            android:name=".MyntraCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".UserRecyclerActivity"
            android:exported="false" />
        <activity
            android:name=".UserDetailActivity"
            android:exported="false" />
        <activity
            android:name=".CustomUserListActivity"
            android:exported="false" />
        <activity
            android:name=".UserDataSimpleListActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
            android:exported="false" />
        <activity
            android:name=".DashboardActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>